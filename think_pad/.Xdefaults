urxvt*scrollBar: false
urxvt*borderless: true
scrollbar styles: rxvt, plain, next or xterm
urxvt*scrollstyle: plain
urxvt*scrollBar_right: true
urxvt*scrollColor: #777777
!urxvt*geometry: 104x31
urxvt*geometry: 103x20
urxvt*saveLines: 10000
urxvt*buffered: true

Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

!urxvt*font: xft:Bitstream Vera Sans Mono:pixelsize=11
urxvt*font: xft:tamsyn:pixelsize=12
!urxvt*font: xft:bitbuntufull:pixelsize=11
!urxvt*font: xft:Ubuntu Mono:pixelsize=12
URxvt*perl-lib: /usr/lib/urxvt/perl
URxvt*perl-ext-common: default,matcher,selection,mark-yank-urls
urxvt*urlLauncher: firefox
urxvt*keysym.M-u: perl:mark-yank-urls:active_mark_mode
urxvt.underlineURLs: true
urxvt*matcher.button: 1
URxvt*matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt*matcher.rend.0: fg6

Xcursor.theme: Vanilla-DMZ-AA
Xcursor.size: 16
