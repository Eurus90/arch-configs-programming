!urxvt*internalBorder: 0
!urxvt*loginShell: true
urxvt*colorMode: on
urxvt*cursorBlink: true
urxvt*cursorUnderline: false
urxvt*cursorColor: #0033ff
urxvt*underlineColor: #8a9bbd
urxvt*highlightColor: #8a9bbd
urxvt*highlightTextColor: #050505

urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0000/dddd
urxvt*background: [35]#000000

URxvt*tintColor: Cyan
URxvt.transparent: true
URxvt.shading: 40

*background: #050505
*foreground: #c0c0c0
!Black
*color0:    #343639
*color8:    #404040
! Red
*color1:    #B28284
*color9:    #BD8A8C
! Green
*color2:    #009900
*color10:   #009900
! Yellow
*color3:    #ff66ff
*color11:   #ff66ff
! Blue
*color4:    #0033ff
*color12:   #0033ff
! Magenta
*color5:    #99FF00
*color13:   #99FF00
! Cyan
*color6:    #00ccff
*color14:   #00ccff
! White
*color7:    #ffffff
*color15:   #ffffff

